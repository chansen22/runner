<h1>Listing runs</h1>

<table>
  <tr>
    <th>Url</th>
    <th>Verified</th>
    <th>Category</th>
    <th>Time in seconds</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @runs.each do |run| %>
  <tr>
    <td><%= run.url %></td>
    <td><%= run.verified %></td>
    <td><%= run.category %></td>
    <td><%= run.time_in_seconds %></td>
    <td><%= link_to 'Show', run %></td>
    <td><%= link_to 'Edit', edit_run_path(run) %></td>
    <td><%= link_to 'Destroy', run, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Run', new_run_path %>
